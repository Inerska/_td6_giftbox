@startuml

title MyGiftBox.net

class Utilisateur {
 - int idUtilisateur
 - String nom
 - String email
 - String motDePasse
}

class Prestation {
 - int idPrestation
 - String nom
 - int prix
 - int categorieId
 - String photo
}

class Coffret {
 - int id
 - String nom
 - String description
 - String cadeau
 - boolean estCadeau
 - String messageCadeau
}

class Categorie {
 - int id
 - String nom
}

class PrestationCoffret {
 - int idPrestationCoffret
 - int quantite
}


class Box {
 - int idBox
 - String nom
}

class Paiement {
 - int idPaiement
 - LocalDate datePaiement
 - float montant
}

class URL {
 - int idURL
 - String url
 - String code
}

Utilisateur "1.." -- "1" Coffret

Utilisateur "1" -- "0.." Paiement

Prestation "1" -- "0.." Categorie

Prestation "0.." -- "0.." PrestationCoffret

Coffret "0..1" -- "1.." PrestationCoffret

Paiement "0..1" -- "1" Coffret

Paiement "1" -- "1" URL

Box "0..1" -- "1.." Prestation

Box "0..1" -- "0.." Coffret

@enduml
